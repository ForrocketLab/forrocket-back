  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\"src/.*\\.spec\\.ts$\"",
    "test:e2e": "jest --testPathPattern=\"\\.e2e-spec\\.ts$\" --runInBand",
    "test:e2e:flows": "jest --testPathPattern=\"evaluations-flow\\.e2e-spec\\.ts$\"",
    "test:e2e:security": "jest --testPathPattern=\"security-integration\\.e2e-spec\\.ts$\"",
    "test:e2e:integration": "jest --testPathPattern=\"evaluations-flow-simple\\.e2e-spec\\.ts$\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --coverageReporters=text-summary --coverageReporters=lcov --coverageReporters=html && pnpm run test:cleanup",
    "test:cov:full": "jest --coverage --verbose && pnpm run test:cleanup",
    "test:cov:html": "jest --coverage --coverageReporters=html && echo \"ðŸ“Š Coverage HTML report: file://$(pwd)/coverage/lcov-report/index.html\" && pnpm run test:cleanup",
    "test:cov:json": "jest --coverage --coverageReporters=json && pnpm run test:cleanup",
    "test:cov:auth": "jest --coverage --testPathPattern=auth --coverageReporters=text --coverageReporters=html && pnpm run test:cleanup",
    "test:cov:watch": "jest --coverage --watch --coverageReporters=text-summary",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:all": "jest --coverage --verbose && pnpm run test:cleanup",
    "test:auth": "jest --testPathPattern=auth",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "db:studio": "prisma studio",
    "db:check": "ts-node scripts/check-database-status.ts",
    "db:cleanup": "ts-node scripts/cleanup-test-data.ts",
    "test:cleanup": "ts-node scripts/post-test-cleanup.ts"
  },
